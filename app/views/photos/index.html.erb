
<div class="row">
  <div class="col-12">
    <div class="nypp-banner">
      <%= image_tag("interview-banner.png") %>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-12">
    <div class="photos-header">
      <h1>New York Pup Pal Interviews</h1>
      <h2>Click a pooch to see more!</h2>
    </div>
  </div>
</div>

<% if signed_in? && current_user.admin? %>
  <p><%= link_to 'New Photo', new_photo_path %></p>
  <p id="notice"><%= notice %></p>
<% end %>

<!-- RENDERS ALL PHOTOS IN INTERVIEWS FROM PARTIAL "_PHOTO" -->
<div class="row">
  <%= render @photos %>
</div>



